		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"mdsvex": "^0.12.5",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"remark-prism": "^1.3.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"unist-util-visit": "^5.0.0",
		"vite": "^6.2.6"
